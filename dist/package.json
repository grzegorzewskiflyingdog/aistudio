{
    "name": "@fds-components/fds-image-editor-canvas",
    "version": "1.1.2",
    "scripts": {
        "build": "npm run build:prepare && rollup -c",
        "devdoc": "npm run build:prepare && export SKIP_HTTPSERVER=true && export MAKE_DOC=true && rollup -c -w",
        "doc": "npm run build:prepare && export SKIP_HTTPSERVER=true && export MAKE_DOC=true && rollup -c && npm run copy:json_doc",
        "dev": "npm run build:prepare  && rollup -c -w",
        "build:prepare": "rm -rf dist && mkdir -p dist/node_modules && npm run copy:json_doc && npm run copy:node_modules",
        "copy:node_modules": "cp -rp node_modules/@fds-components dist/node_modules/@fds-components",
        "copy:pages": "cp -rp dist public",
        "copy:json_doc": "find . -maxdepth 1 -name \"*.json\" ! -name 'package*' ! -name 'COMPONENT_API.json' -type f  -exec cp -- {} dist \\;",
        "distcomfy": "node scripts/serverpackdist.js",
        "cc": "node ./helper/cc.js",
        "runbin": "node ./bin/create-app.js $ARG",
        "start": "sirv dist --no-clear --port 3333 --host"
    },
    "files": [
        "dist/*.js*",
        "dist/*.json*",
        "bin/*.js*"
    ],
    "devDependencies": {
        "@capacitor/assets": "^2.0.4",
        "@capacitor/cli": "^5.3.0",
        "@fds-components/fds-helper": "^1.0.9",
        "@fds-components/fds-icon": "^1.1.7",
        "@fds-components/fds-md-documenter": "^1.0.23",
        "@rollup/plugin-commonjs": "17.1.*",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "15.1.*",
        "copy-dir": "^1.3.0",
        "eslint": "^8.39.0",
        "eslint-plugin-svelte": "^2.32.4",
        "rollup": "2.*",
        "rollup-plugin-copy": "3.4.*",
        "rollup-plugin-css-only": "^3.0.0",
        "rollup-plugin-livereload": "2.0.*",
        "rollup-plugin-svelte": "^7.1.5",
        "rollup-plugin-terser": "7.0.*",
        "sirv-cli": "1.0.*",
        "sveld": "^0.10.2",
        "svelte": "^4.0.0",
        "svelte-dnd-action": "^0.9.11",
        "svelte-loading-spinners": "^0.3.4"
    },
    "svelte": "./src/main.js",
    "dependencies": {
        "@capacitor/core": "^5.3.0",
        "@capacitor/ios": "^5.3.0",
        "@fds-components/fds-3d-scene": "^1.0.16",
        "@fds-components/fds-ai-image-editor": "^1.1.70",
        "@fds-components/fds-image-editor-button": "^1.0.33",
        "@fds-components/fds-image-editor-dialog": "^1.0.16",
        "@fds-components/fds-image-editor-text": "^1.0.20",
        "@fds-components/fds-image-transform": "^1.0.12",
        "@fds-components/fds-local-files": "^1.0.15",
        "@fds-components/fds-pose-editor": "^1.0.39",
        "@fds-components/fds-progress": "^1.0.4",
        "@fds-components/fds-slider": "^1.0.6",
        "@fds-components/fds-tabs": "^1.0.19",
        "@fds-components/fds-tippy": "^1.0.14",
        "@fds-components/fds-treeview": "^1.0.52",
        "@imagemagick/magick-wasm": "0.0.21",
        "@zip.js/zip.js": "^2.7.6",
        "arrow-line": "^0.7.5",
        "canvaskit-wasm": "^0.38.2",
        "form-data": "^4.0.0",
        "image-js": "^0.35.3",
        "interactjs": "^1.10.17",
        "just-clone": "^6.2.0",
        "pica": "^9.0.1",
        "request": "^2.88.2",
        "sweetalert": "^2.1.2",
        "sweetalert2": "^11.7.3"
    }
}
